#include <LiquidCrystal.h>

// (RS, E, D4, D5, D6, D7)
LiquidCrystal lcd(4, 5, 14, 12, 13, 15);

int contador = 0;

void setup() {
  lcd.begin(16, 2);
  delay(500);
}

void loop() {
  lcd.clear();
  lcd.setCursor(0, 0);
  lcd.print("Exemplo LCD !");
  lcd.setCursor(1, 1);
  lcd.print(contador);
  contador++;
  if (contador == 60) contador = 0;
  delay(1000);
}
